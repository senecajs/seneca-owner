{
  "name": "@seneca/owner",
  "version": "4.0.1",
  "description": "Seneca plugin to add user ownership annotations to entities.",
  "main": "owner.js",
  "scripts": {
    "test": "lab -v -P test -t 80 -I BigUint64Array,BigInt64Array,BigInt,URL,URLSearchParams",
    "prettier": "prettier --write --no-semi --single-quote *.js lib/*.js test/*.js",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -r html > coverage.html",
    "clean-npm": "rm -rf node_modules package-lock.json",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run prettier && npm test && npm run repo-tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voxgig/seneca-owner.git"
  },
  "author": "Richard Rodger (richardrodger.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voxgig/seneca-owner/issues"
  },
  "homepage": "https://github.com/voxgig/seneca-owner#readme",
  "files": [
    "LICENSE",
    "README.md",
    "owner.js"
  ],
  "dependencies": {
    "@hapi/joi": "^15.1.1"
  },
  "devDependencies": {
    "@hapi/code": "^6.0.0",
    "@hapi/lab": "^20.2.2",
    "coveralls": "^3.0.6",
    "prettier": "^1.18.2",
    "seneca": "^3.15.1",
    "seneca-entity": "^5.1.2",
    "seneca-plugin-validator": "^0.3.0",
    "seneca-promisify": "^1.1.0"
  }
}
